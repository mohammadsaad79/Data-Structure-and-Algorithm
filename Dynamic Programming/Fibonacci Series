#include<bits/stdc++.h>
using namespace std;

long long fib(int n) {
    if(n == 0 || n == 1) return 1;
    int dp[n];
    dp[0] = 0;
    dp[1] = 1;
    for(int i = 2; i <= n; i++)
        dp[i] = dp[i - 1] + dp[i - 2];
    return dp[n];
}

int main() {
    int n;
    cout <<"Enter nth number: ";
    cin >> n;
    cout << "The "<< n <<"th fibonacci number is: " << fib(n) << endl;
    return 0;
}
