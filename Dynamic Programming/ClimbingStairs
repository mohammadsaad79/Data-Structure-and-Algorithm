#include<bits/stdc++.h>
using namespace std;

int numOfWays(int n) {
    int dp[n];
    dp[0] = 1;
    dp[1] = 1;
    for(int i = 2; i <= n; i++)
        dp[i] = dp[i - 1] + dp[i - 2];
    return dp[n];
}

int main() {
    int n;
    cout << "Enter number of steps: ";
    cin >> n;
    cout << "There are " << numOfWays(n) << " distinct ways" << endl;
    return 0;
}
