#include<bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cout << "Enter any number: ";
    cin >> n;
    int sum = 0;
    while(n != 0) {
        int last_digit = n % 10;
        sum += last_digit;
        n /=10;
        if(n == 0) {
            n = sum;
            sum = 0;
            if(n < 10)
                break;
        }
    }
    cout << "Sum: " << n;
}
