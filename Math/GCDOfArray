#include<bits/stdc++.h>
using namespace std;

int manualGcd(int a, int b) {
    while(b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int gcd(int arr[], int n) {
    int result = arr[0];
    for(int i = 1; i < n; i++) {
        result = manualGcd(result, arr[i]);
        if(result == 1) break;
    }
    return result;
}

int main() {
    int arr[] = {36, 21, 30, 18, 24};
    int n = sizeof(arr) / sizeof(arr[0]);
    int result = gcd(arr, n);
    cout << result << endl;
}
